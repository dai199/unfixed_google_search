{
  "manifest_version": 2,

  "name": "Unfixed google search form",
  "description" : "Unfixed google search form and enable a page to see expanded screen.",
  "version": "1.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "content_scripts": [
    {
      "matches": ["*://www.google.com/search*"],
      "css": ["style.css"]
    }
  ]
}
